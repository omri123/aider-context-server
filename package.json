{
  "name": "aider-context-server",
  "displayName": "aider",
  "description": "Context server for aider - AI pair programming in your terminal",
  "version": "0.0.1",
  "publisher": "omribloch",
  "license": "Apache-2.0",
  "homepage": "https://github.com/omri123/aider-context-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/omri123/aider-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/omri123/aider-vscode/issues"
  },
  "keywords": [
    "gpt",
    "chatGPT",
    "assistant",
    "aider",
    "gpt4",
    "agent"
  ],
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aider-context-server.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
